{
  "name": "nextjs-blog-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate-api": "dotenv -e .env -- sh -c 'npx swagger-typescript-api -p $NEXT_PUBLIC_API_URL/api-json -o ./api -n document.ts'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "postinstall": "npm run generate-api"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "effector": "^23.2.2",
    "effector-react": "^23.2.1",
    "effector-storage": "^7.1.0",
    "eslint-plugin-effector": "^0.14.0",
    "framer-motion": "^11.3.28",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-intersection-observer": "^9.13.0",
    "react-toastify": "^10.0.5",
    "react-toggle-dark-mode": "^1.1.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "daisyui": "^4.12.10",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix --max-warnings 0"
    ],
    "*.{css,md}": "prettier --write"
  }
}
